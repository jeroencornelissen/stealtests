{
  "name": "can-event-radiochange",
  "description": "The custom \"radiochange\" event",
  "version": "0.0.2",
  "author": {
    "name": "Bitovi",
    "email": "core@donejs.com",
    "url": "https://bitovi.com"
  },
  "browser": {
    "transform": [
      "cssify"
    ]
  },
  "browserify": {
    "transform": [
      "cssify"
    ]
  },
  "bugs": {
    "url": "https://github.com/canjs/can-event-radiochange/issues"
  },
  "devDependencies": {
    "can-util": "^3.4.1",
    "done-serve": "^1.0.0",
    "donejs-cli": "^1.0.0",
    "fixpack": "^2.3.1",
    "generator-donejs": "^1.0.0",
    "jshint": "^2.9.1",
    "steal": "^1.3.1",
    "steal-qunit": "^1.0.1",
    "steal-tools": "^1.2.0",
    "testee": "^0.3.0"
  },
  "homepage": "http://canjs.com",
  "keywords": [
    "canjs",
    "canjs-plugin",
    "donejs"
  ],
  "license": "MIT",
  "main": "dist/cjs/can-event-radiochange",
  "repository": {
    "type": "git",
    "url": "git://github.com/canjs/can-event-radiochange.git"
  },
  "scripts": {
    "build": "node build.js",
    "develop": "done-serve --static --develop --port 8080",
    "jshint": "jshint ./*.js ./src/ --config",
    "lint": "fixpack && npm run jshint",
    "postpublish": "git push --tags && git checkout master && git branch -D release && git push",
    "preversion": "npm test && npm run build",
    "release:major": "npm version major && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:patch": "npm version patch && npm publish",
    "test": "npm run lint && npm run testee",
    "testee": "testee test.html --browsers firefox",
    "version": "git commit -am \"Update version number\" && git checkout -b release && git add -f dist/"
  },
  "steal": {
    "main": "can-event-radiochange",
    "configDependencies": [
      "live-reload"
    ],
    "npmIgnore": [
      "testee",
      "generator-donejs",
      "donejs-cli",
      "steal-tools"
    ],
    "directories": {
      "lib": "src"
    }
  }
}
