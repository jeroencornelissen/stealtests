{
  "name": "transpile",
  "description": "Transpiles JavaScript modules from one format to another.",
  "version": "2.3.0",
  "author": {
    "name": "Bitovi",
    "email": "contact@bitovi.com",
    "web": "http://bitovi.com/"
  },
  "dependencies": {
    "babel-standalone": "^6.23.1",
    "comparify": "0.2.0",
    "escodegen": "^1.7.0",
    "esprima": "^3.1.2",
    "estraverse": "4.2.0",
    "js-module-formats": "~0.1.2",
    "js-string-escape": "1.0.1",
    "object.assign": "^4.0.1",
    "sourcemap-to-ast": "0.0.2",
    "traceur": "0.0.111",
    "urix": "^0.1.0"
  },
  "devDependencies": {
    "babel-preset-steal-test": "0.0.1",
    "chai": "*",
    "istanbul": "*",
    "mocha": "3.2.0",
    "xunit-file": "*"
  },
  "globalBrowser": {
    "traceur#./src/node/traceur.js": "./browser/traceur.js"
  },
  "system": {
    "meta": {
      "traceur#./bin/traceur": {
        "format": "global"
      }
    }
  },
  "scripts": {
    "mocha": "mocha test/*.js",
    "test": "./node_modules/istanbul/lib/cli.js cover -- ./node_modules/.bin/_mocha test/*.js --reporter spec",
    "test-windows": "node_modules\\.bin\\mocha test\\*.js --reporter spec"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/mit-license.php"
    }
  ],
  "main": "main",
  "repository": {
    "type": "git",
    "url": "git://github.com/stealjs/transpile.git"
  }
}
